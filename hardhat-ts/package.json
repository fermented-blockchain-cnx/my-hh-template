{
    "name": "@nazt/nft-contracts",
    "version": "0.0.3",
    "description": "nazt solidity contracts & artifacts",
    "author": "Nazt",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "scripts": {
        "compile": "hardhat compile",
        "test": "hardhat test",
        "deploy": "hardhat deploy",
        "node": "hardhat node",
        "webconfig:mumbai": "mustache generated/deployments/mumbai/MyNFT.json ../nft-web/src/config/addresses.template.ts > ../nft-web/src/config/addresses.ts",
        "webconfig:goerli": "mustache generated/deployments/goerli/MyNFT.json ../nft-web/src/config/addresses.template.ts > ../nft-web/src/config/addresses.ts",
        "webconfig:rinkeby": "mustache generated/deployments/rinkeby/MyNFT.json ../nft-web/src/config/addresses.template.ts > ../nft-web/src/config/addresses.ts",
        "build": "yarn run hardhat export-abi && yarn build:sol && yarn build:ts && yarn build:declarations",
        "build:ts": "tsc -p tsconfig.json",
        "build:sol": "hardhat compile",
        "build:declarations": "echo 'hello' /*shx cp typechain/*.ts dist/typechain*/",
        "lint:fix": "prettier --write 'scripts/**/*.{js,ts}' 'test/**/*.{js,ts}' 'tasks/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json && solhint contracts/**/*.sol",
        "lint": "tslint --config tslint.json --project tsconfig.json && solhint contracts/**/*.sol",
        "format": "prettier --check \"**/*.{ts,js,sol}\"",
        "format:fix": "prettier --write \"**/*.{ts,js,sol}\""
    },
    "files": [
        "dist/src/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.0",
        "@nomiclabs/hardhat-etherscan": "^3.0.0",
        "@nomiclabs/hardhat-solhint": "^2.0.0",
        "@nomiclabs/hardhat-waffle": "^2.0.2",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@openzeppelin/contracts": "^4.4.2",
        "@openzeppelin/hardhat-upgrades": "^1.14.0",
        "@pinata/sdk": "^1.1.23",
        "@typechain/ethers-v5": "^9.0.0",
        "@typechain/hardhat": "^4.0.0",
        "@types/chai": "^4.3.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.15",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.6",
        "chai-bignumber": "^3.0.0",
        "chai-bn": "^0.3.1",
        "chai-ethers": "^0.0.1",
        "dotenv": "^16.0.0",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.5.4",
        "hardhat": "^2.8.3",
        "hardhat-abi-exporter": "^2.8.0",
        "hardhat-deploy": "^0.10.4",
        "hardhat-gas-reporter": "^1.0.7",
        "keccak256": "^1.0.6",
        "merkletreejs": "^0.2.27",
        "mocha-skip-if": "^0.0.3",
        "mustache": "^4.2.0",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.0.0-beta.19",
        "qrcode-terminal": "^0.12.0",
        "shx": "^0.3.4",
        "solhint": "^3.3.6",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.7.18",
        "transform": "^1.1.2",
        "ts-generator": "^0.1.1",
        "ts-node": "^10.4.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.3.0",
        "typechain": "^7.0.0",
        "typescript": "^4.5.5",
        "@tenderly/hardhat-tenderly": "^1.0.13",
        "hardhat-watcher": "^2.1.1",
        "web3": "^1.7.0"
    },
    "mocha": {
        "timeout": 10000000
    }
}
